{
  "name": "@cactuslab/native-navigation-react-router",
  "version": "3.0.0",
  "description": "React Router support for Native navigation for Capacitor apps",
  "main": "dist/plugin.cjs.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "unpkg": "dist/plugin.js",
  "files": [
    "dist/"
  ],
  "exports": {
    ".": "./dist/esm/index.js"
  },
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cactuslab/capacitor-native-navigation.git"
  },
  "bugs": {
    "url": "https://github.com/cactuslab/capacitor-native-navigation/issues"
  },
  "keywords": [
    "capacitor",
    "plugin",
    "native"
  ],
  "scripts": {
    "lint": "pnpm run eslint && pnpm run prettier -- --check",
    "fmt": "pnpm run eslint -- --fix && pnpm run prettier -- --write",
    "eslint": "eslint . --ext ts",
    "prettier": "prettier \"**/*.{css,html,ts,js,java}\"",
    "build": "pnpm run clean && tsc && rollup -c rollup.config.mjs",
    "clean": "rimraf ./dist",
    "watch": "tsc --watch",
    "prepublishOnly": "pnpm run build"
  },
  "dependencies": {
    "@cactuslab/native-navigation-react": "^3.0.0"
  },
  "devDependencies": {
    "@cactuslab/native-navigation": "^0.3.0",
    "@capacitor/core": "^4.8.0",
    "@ionic/eslint-config": "^0.3.0",
    "@ionic/prettier-config": "^2.1.2",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.1",
    "eslint": "^8.39.0",
    "prettier": "~2.8.8",
    "react-router-dom": "^6.11.0",
    "rimraf": "^5.0.0",
    "rollup": "^3.21.3",
    "typescript": "~5.0.4"
  },
  "peerDependencies": {
    "@cactuslab/native-navigation": "^0.3.0",
    "@capacitor/core": "^4.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.4.3"
  },
  "prettier": "@ionic/prettier-config",
  "eslintConfig": {
    "extends": "@ionic/eslint-config/recommended"
  }
}
